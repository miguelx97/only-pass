    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron:win": "electron-packager ./electron OnlyPass --overwrite --asar --platform=win32 --arch=x64 --prune=true --out=desktop-release --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='OnlyPass' --icon='./electron-assets/icons/desktop-icon.ico'",
    "electron:mac": "electron-packager ./electron OnlyPass --overwrite --platform=darwin --arch=x64 --prune=true --out=desktop-release --icon='./electron-assets/icons/desktop-icon.ico'",
    "electron:linux": "electron-packager ./electron OnlyPass --overwrite --asar --platform=linux --arch=x64 --icon='./electron-assets/icons/desktop-icon.ico' --prune=true --out=desktop-release",
    "electron:debian": "electron-installer-debian --src desktop-release/OnlyPass-linux-x64/ --dest desktop-release --arch amd64 --icon electron-assets/icons/desktop-icon.png"
  },
  "private": true,
